{
  "name": "@budarin/task-queue",
  "version": "1.0.4",
  "description": "Task queue",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "repository": "https://github.com/budarin/task-queue.git",
  "author": "Vadim Budarin <budarin-vv@yandex.ru>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.6.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "npm-publish": "npm run build && npm publish",
    "upgrade deps": "yarn upgrade-interactive --latest;",
    "show what will be published": "npm pack --dry-run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  }
}