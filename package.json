{
  "name": "@budarin/task-queue",
  "version": "1.0.13",
  "description": "Task queue",
  "repository": "https://github.com/budarin/task-queue.git",
  "author": "Vadim Budarin <budarin-vv@yandex.ru>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "module": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "types": "./dist/types/*.d.ts",
      "default": "./dist/cjs/*.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "npm-publish": "npm run build && npm publish",
    "upgrade deps": "yarn upgrade-interactive --latest;",
    "show what will be published": "npm pack --dry-run"
  }
}